   "source": [
    "R.<x>  = PolynomialRing(GF(2))\n",
    "FF.<a> = FiniteField(2^4, modulus=x^4+x+1)\n",
    "RR.<y> = PolynomialRing(FF)\n",
    "\n",
    "# Defining set: 1, 2, 3, 4, 5\n",
    "g = a.minimal_polynomial()*(a^3).minimal_polynomial()*(a^5).minimal_polynomial()\n",
    "g"
   ]
